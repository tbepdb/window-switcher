<schemalist gettext-domain="gnome-shell-extensions">
  <schema id="org.gnome.shell.extensions.window-switcher" path="/org/gnome/shell/extensions/window-switcher/">
    <key name="application-list" type="as">
      <!-- FIXME: should be a(su), when JS supports more of GVariant -->
      <default>[ ]</default>
      <summary>Application list</summary>
      <description>A list of strings, each containing an application id (desktop file name)</description>
    </key>
    <key name="show-minimized" type="b">
      <default>true</default>
    </key>
    <key name="all-desktops" type="b">
      <default>false</default>
    </key>
  </schema>
</schemalist>
